{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/index.js" },
    { "source": "/proxy-image(.*)", "destination": "/index.js" },
    { "source": "/health(.*)", "destination": "/index.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}